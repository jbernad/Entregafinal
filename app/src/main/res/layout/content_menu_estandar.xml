<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A9F5F2"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.jorgebr.pglfinal.MenusDrawer.MenuEstandarActivity"
    tools:showIn="@layout/app_bar_menu_estandar">
    <!--TextView con id del user-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/idprueba"
        android:hint="iduser"
        android:textSize="16dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="56dp" />
    <ImageView
        android:layout_width="300dp"
        android:background="@mipmap/ic_userestandard"
        android:layout_height="300dp"
        android:layout_marginBottom="63dp"
        android:id="@+id/imageView2"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
