<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A9F5F2"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.jorgebr.pglfinal.MenusDrawer.MenuDrawerActivity"
    tools:showIn="@layout/app_bar_menu_drawer">
    <!--FECHAS-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="fecha"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="54dp"
        android:layout_marginTop="72dp"
        android:id="@+id/textView2">
    </TextView>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Introduzca fecha busqueda"
        android:id="@+id/editfecha"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_toEndOf="@+id/textView3"
        android:layout_marginStart="16dp" />
    <!--IDUSER-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cliente"
        android:layout_alignParentTop="true"
        android:layout_marginTop="112dp"
        android:layout_alignStart="@+id/textView2"
        android:id="@+id/textView3">
    </TextView>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="Introduzca IDUSER buscar"
        android:id="@+id/iduser"
        android:layout_below="@+id/editfecha"
        android:layout_alignStart="@+id/editfecha" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/iduser"
        android:layout_marginTop="18dp"
        app:srcCompat="@mipmap/ic_admin" />
</RelativeLayout>
